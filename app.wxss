/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.clear::after{
  content: "";
  height: 0;
  line-height: 0;
  display: block;
  visibility: hidden;
  clear: both;
  zoom: 1;
}
.motto {
  width: 100%;
  padding: 30rpx 0;
  color: #333;
  font-size: 26rpx;
  text-align: center;
  background-color: white;
}
