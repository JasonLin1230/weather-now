/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.clear::after{
  content: "";
  height: 0;
  line-height: 0;
  display: block;
  visibility: hidden;
  clear: both;
  zoom: 1;
}
.motto {
  width: 100%;
  padding: 30rpx 0;
  color: #333;
  font-size: 26rpx;
  text-align: center;
  background-color: white;
}
/* contact */
.contact{
  width: 100%;
  height: 140rpx;
  background-color: #eeeeee;
}
.contact-container{
  width: 360rpx;
  margin: 0 auto;
}
.contact-container .contact-btn{
  height: 140rpx;
  line-height: 140rpx;
  display: inline-block;
  font-size: 24rpx;
  width: 180rpx;
  background-color: #eeeeee;
  color: #888888;
  border: 0px;
  border-radius: 0px;
}
