.locate_btn{
  border: none;
  color: #878787;
  cursor: pointer;
  background: white;
  font-size: 35rpx;
  overflow: hidden;
  white-space:nowrap;
  padding: 0;
}
.locate_btn::after{
  border: none;
  position: relative;
}
.update_loc{
  margin-top: 100rpx;
  font-size: 28rpx;
  color: #aaa;
}
.main_section{
  display: flex;
  margin-top: 30rpx;
}
.cond_icon{
  width: 200rpx;
  float: left;
}
.cond_icon>image{
  width: 200rpx;
  height: 200rpx;
}
.cond_detail{
  width: 260rpx;
  margin-left: 60rpx;
  float: left;
}
.cond_txt{
  display: block;
  font-size: 65rpx;
  text-align: center;
  margin-bottom: 30rpx;
  color: #333;
}
.cond_type{
  width: 50%;
  float: left;
  font-size: 40rpx;
  color: #333;
  text-align: center;
}
.cond_type>text{
  color: #aaa;
  display: block;
  font-size: 33rpx;
}
.forecast_section{
  margin-top: 70rpx;
}
.small_icon{
  width: 90rpx;
  height: 90rpx;
  vertical-align: middle;
}
.fore text{
  display: inline-block;
  width: 128rpx;
  text-align: center;
  color: #878787;
  font-size: 32rpx;
  padding: 0 18rpx;
}
.fore .fore_day{
  width: auto;
  margin-right: 38rpx;
  padding-right: 32rpx;
  border-right: 1px solid #878787;
}
.cond_tip{
  margin-top: 70rpx;
}
.container .tip{
  display: block;
  text-align: center;
  font-size: 28rpx;
  color: #aaa;
  margin-bottom: 26rpx;
}
.tip_detail{
  font-size: 32rpx;
  color: #333;
}
.motto {
  padding: 10rpx 0;
  position: absolute;
  bottom: 0;
  color: #333;
  font-size: 26rpx;
}